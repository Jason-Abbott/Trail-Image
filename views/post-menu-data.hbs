{{!
   Generate Javscript data structure for menu

   http://www.cs.tut.fi/~jkorpela/chars/spaces.html

   Rendered to
   http://www.trailimage.com/js/post-menu-data.js
}}
var postMenuData = {
   category: {
{{#each blog.categories}}
      '{{this.title}}': [{{#each this.subcategories}} {
            title: '{{this.title}}',
            posts: [{{#each this.posts}}'{{this.id}}'{{#unless @last}},{{/unless}}{{/each}}]
         },{{/each}}],
{{/each}}
   },
   post: { {{#each blog.posts}}
      '{{this.id}}': {
         slug: '{{this.key}}',
         title: '{{this.title}}',
         icon: '{{modeIconForPost this.categories}}',
         description: '{{this.description}}'{{#this.isPartial}},
         subTitle: '{{this.subTitle}}',
         part: {{this.part}}{{/this.isPartial}}
      }{{#unless @last}},{{/unless}}{{/each}}
   }
};