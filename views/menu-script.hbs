{{! http://www.cs.tut.fi/~jkorpela/chars/spaces.html }}
var menu = {};
{{#each library.tags}}
	menu['{{this.title}}'] = [
	{{#this.tags}}
		{ 'title': '{{this.title}}', "items":[
		{{#this.posts}}
			{{#if this.isPartial}}
				{{#if this.isSeriesStart}}
					{{! these are in reverse order because Flickr originally returned them newest-first }}
					{ 'slug': '{{this.slug}}', 'title': '{{this.subTitle}}', 'part': {{this.part}} },
					{ 'slug': '{{this.slug}}', 'title': '{{this.title}}', 'icon': '{{this.mode}}' },
				{{else}}
					{ 'slug': '{{this.slug}}', 'title': '{{this.subTitle}}', 'part': {{this.part}} },
				{{/if}}
			{{else}}
				{ 'slug': '{{this.slug}}', 'title': '{{this.title}}', 'icon': '{{this.mode}}' },
			{{/if}}
		{{/this.posts}}
		]},
	{{/this.tags}}
	];
{{/each}}