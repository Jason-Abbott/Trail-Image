{{! http://www.cs.tut.fi/~jkorpela/chars/spaces.html }}
var menu = {};
{{#each library.tags}}
	menu['{{this.title}}'] = [
	{{#this.tags}}
		{ 'title': '{{this.title}}', "items":[
		{{#this.posts}}
			{ 'slug': '{{this.slug}}', 'title': '{{this.title}}', 'icon': '{{this.mode}}', 'description': '{{this.description}}'
			{{#this.isPartial}} , 'subTitle': '{{this.subTitle}}', 'part': {{this.part}} {{/this.isPartial}} },
		{{/this.posts}}
		]},
	{{/this.tags}}
	];
{{/each}}