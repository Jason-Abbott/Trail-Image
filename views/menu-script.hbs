{{! http://www.cs.tut.fi/~jkorpela/chars/spaces.html }}
var menu = {};
{{#each library.tags}}
	menu['{{this.title}}'] = [
	{{#this.tags}}
		{ 'title': '{{this.title}}', "items":[
		{{#this.posts}}
			{{#if this.isSeriesStart}}
				{ 'slug': '{{this.slug}}', 'title': '{{this.title}} ({{this.totalParts}} parts)', 'icon': '{{this.mode}}' },
			{{/if}}

			{{^this.isPartial}}
				{ 'slug': '{{this.slug}}', 'title': '{{this.title}}', 'icon': '{{this.mode}}' },
			{{/this.isPartial}}
		{{/this.posts}}
		]},
	{{/this.tags}}
	];
{{/each}}