<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<url>
		<loc>http://www.{{config.domain}}/</loc>
		<changefreq>daily</changefreq>
		<priority>1</priority>
	</url>
	<url>
		<loc>http://www.{{config.domain}}/about</loc>
		<changefreq>monthly</changefreq>
		<priority>0.7</priority>
	</url>
{{#each posts}}
	<url>
		<loc>http://www.{{../config.domain}}/{{this.slug}}</loc>
		<changefreq>monthly</changefreq>
	</url>
{{/each}}
{{#each tags}}
	<url>
		<loc>http://www.{{../config.domain}}/{{this}}</loc>
		<changefreq>monthly</changefreq>
	</url>
{{/each}}
{{#each photoTags}}
	<url>
		<loc>http://www.{{../config.domain}}/photo-tag/{{@key}}</loc>
		<changefreq>monthly</changefreq>
	</url>
{{/each}}
</urlset>