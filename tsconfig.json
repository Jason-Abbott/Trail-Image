{
   "compilerOptions": {
      "target": "es2015",
      "module": "commonjs",
      "baseUrl": "./src",
      "newLine": "LF",
      "noImplicitAny": true,
      "allowJs": false,
      "alwaysStrict": true,
      "strictNullChecks": false,
      "allowSyntheticDefaultImports": false,
      "forceConsistentCasingInFileNames": true,
      "typeRoots": ["node_modules/@types"]
   }
}