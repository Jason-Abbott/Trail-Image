{
   "css.validate": false,
   "editor.fontFamily": "Fira Code",
   "editor.fontLigatures": true,
   "editor.formatOnSave": true,
   "editor.tabSize": 3,
   "files.associations": {
      "procfile": "yaml"
   },
   "files.eol": "\n",
   "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "lib/**/.js.map": true
   },
   "typescript.referencesCodeLens.enabled": false,
   "search.exclude": {
      "**/node_modules": true,
      "**/lib": true,
      "**/dist": true
   },
   "tslint.enable": true,
   "eslint.enable": false
}