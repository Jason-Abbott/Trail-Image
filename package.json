{
   "name": "trail-image",
   "description": "Trail Image blog",
   "version": "3.0.6",
   "private": false,
   "author": {
      "name": "Jason Abbott"
   },
   "license": "MIT",
   "engines": {
      "node": ">=9.1.0"
   },
   "main": "lib/app.js",
   "scripts": {
      "test-pretty":
         "node_modules/.bin/mocha --recursive --reporter mocha-pretty-spec-reporter",
      "test": "node_modules/.bin/mocha --recursive --reporter spec",
      "tstest": "node_modules/.bin/mocha -r ts-node/register",
      "watch": "node_modules/.bin/gulp watch",
      "ts": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/app.ts",
      "webfont":
         "webfont-dl \"http://fonts.googleapis.com/css?family=Oswald:400,700|Lora:400,400italic'\" --css-rel=./fonts -o dist/fonts/webfont.css",
      "renamed_from_postinstall":
         "yarn add git+https://github.com/wrr/connect-injector.git"
   },
   "dependencies": {
      "body-parser": "^1.18.0",
      "compression": "^1.7.0",
      "connect-injector-frk": "git+https://github.com/wrr/connect-injector.git",
      "connect-wwwhisper": "^0.1.18",
      "express": "^4.16.0",
      "express-hbs": "^1.0.0",
      "feed": "^1.1.0",
      "google-auth-library": "^1.1.0",
      "googleapis": "^25.0.0",
      "jszip": "^3.1.3",
      "node-fetch": "^1.7.1",
      "oauth": "^0.9.0",
      "pdfkit": "^0.8.2",
      "redis": "^2.8.0",
      "uglify-js": "^3.3.0",
      "winston": "^2.4.0",
      "winston-redis": "^1.0.0",
      "xmldom": "^0.1.19"
   },
   "devDependencies": {
      "@toba/lint": "^1.2.0",
      "@toba/test": "^1.0.0",
      "@toba/utility": "^1.5.3",
      "@types/body-parser": "^1.16.3",
      "@types/compression": "0.0.35",
      "@types/core-js": "^0.9.41",
      "@types/express": "^4.0.35",
      "@types/facebook-js-sdk": "^2.8.2",
      "@types/gapi": "0.0.35",
      "@types/geojson": "^1.0.1",
      "@types/google.analytics": "0.0.36",
      "@types/googlemaps": "^3.26.13",
      "@types/jquery": "^3.2.6",
      "@types/jszip": "3.1.0",
      "@types/mapbox-gl": "^0.40.0",
      "@types/node-fetch": "^1.6.7",
      "@types/redis": "^2.8.0",
      "@types/request": "^2.0.0",
      "@types/twitter": "0.0.28",
      "@types/uglify-js": "^2.6.29",
      "@types/winston": "^2.3.3",
      "@types/xmldom": "^0.1.29",
      "gulp": "^3.9.1",
      "gulp-concat": "^2.6.1",
      "gulp-cssnano": "^2.1.2",
      "gulp-less": "^3.4.0",
      "gulp-sourcemaps": "^2.6.0",
      "gulp-typescript": "^3.1.7",
      "gulp-uglify": "^3.0.0",
      "merge2": "^1.0.3",
      "nodemon": "^1.12.0",
      "through2": "^2.0.3",
      "ts-node": "^4.1.0",
      "tslint": "^5.8.0",
      "typescript": "^2.6.0",
      "vinyl": "^2.1.0"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/Jason-Abbott/trail-image.git"
   },
   "bugs": {
      "url": "https://github.com/Jason-Abbott/trail-image/issues"
   },
   "prettier": {
      "arrowParens": "avoid",
      "bracketSpacing": true,
      "printWidth": 80,
      "semi": true,
      "singleQuote": true,
      "tabWidth": 3,
      "trailingComma": "none",
      "useTabs": false
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "transformIgnorePatterns": ["<rootDir>/node_modules/(?!@toba)"],
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
   },
   "stylelint": {
      "plugins": ["@toba/lint/style"]
   }
}
